{
  "family": "etoe-task-def",
  "executionRoleArn": "arn:aws:iam::325511643680:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["EC2"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "etoe-app",
      "image": "325511643680.dkr.ecr.ap-south-1.amazonaws.com/etoe/app:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}

